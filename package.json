    "ioredis": "^5.4.1",
    "lodash-es": "^4.17.21",
    "socket.io-client": "^4.8.0"
  },
  "devDependencies": {
    "@kikiutils/classes": "^3.0.0",
    "@kikiutils/hyper-express": "^6.22.0",
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/nodemailer": "^6.4.16",
    "changelogen": "^0.5.7",
    "nodemailer": "^6.9.15",
