{
  "name": "package-template",
  "type": "module",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/author-or-organization/repo-name.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix --max-warnings 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "tsc -b --noEmit",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "dependencies": {},
  "devDependencies": {
    "@kikiutils/changelogen": "^0.9.0",
    "@kikiutils/eslint-config": "^3.0.1",
    "@kikiutils/tsconfigs": "^5.0.5",
    "@types/node": "^24.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "depcheck": "^1.4.7",
    "publint": "^0.3.12",
    "ts-unused-exports": "^11.0.1",
    "tsconfck": "^3.1.6",
    "tsdown": "^0.15.1",
    "typescript": "^5.9.2",
    "unplugin-unused": "^0.5.3",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
